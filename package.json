    "gatsby-plugin-mdx": "^1.2.34",
    "gatsby-plugin-offline": "^3.2.22",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-trailing-slashes": "^2.3.11",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.25",
    "gatsby-plugin-smoothscroll": "^1.2.0",
    "gatsby-plugin-typescript": "^2.4.17",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-images": "^3.3.25",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-sharp": "^2.5.12",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "description": "Christopher Obando Portfolio Website",
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "gatsby",
    "typescript",
    "nodejs",
    "mdx",
    "mdxjs"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ],
    "src/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "name": "obando-personal-site-v4",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/chrisjobando/personal-website-v4"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "format": "npm run lint:fix",
    "lint": "eslint . --ext .js,.json,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.json,.jsx,.ts,.tsx  --fix",
    "lint:staged": "lint-staged",
    "prettier": "prettier --write '*.{js,ts,jsx,tsx,json,md}'",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "version": "0.1.0"
}
