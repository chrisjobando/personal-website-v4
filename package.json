    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "format": "yarn lint:fix",
    "lint": "eslint . --ext .js,.json,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.json,.ts,.tsx  --fix",
    "lint:staged": "lint-staged",
    "prettier": "prettier --write '*.{js,ts,jsx,tsx,json,md,mdx}'",
